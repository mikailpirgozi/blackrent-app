    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.18",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.539.0",
    "next": "^14.2.18",
    "postcss": "^8.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
