{
  "name": "blackrent-customer-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "dev:safe": "./scripts/safe-dev.sh",
    "dev:stable": "./scripts/start-dev.sh",
    "dev:stop": "./scripts/stop-dev.sh",
    "dev:clean": "rm -rf .next && npm run dev",
    "build": "next build",
    "build:clean": "rm -rf .next && npm run build",
    "start": "next start -p 3002",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
