{
  "name": "blackrent-backend",
  "version": "1.0.0",
  "description": "Backend pre Blackrent aplikáciu",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon index.ts",
    "start": "node src/index.js",
    "start-ts": "ts-node index.ts",
    "build": "tsc",
    "setup": "./setup-postgres.sh",
    "setup-db": "psql -U postgres -c \"CREATE DATABASE blackrent;\" 2>/dev/null || echo \"Databáza už existuje\"",
    "backup": "node backup.js",
    "restore": "node restore.js",
    "list-backups": "ls -la backups/",
    "test-data": "node create-test-data.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "blackrent",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
