{
  "name": "blackrent-backend",
  "version": "1.0.0",
  "description": "Backend pre Blackrent aplikáciu",
  "main": "dist/fastify-server.js",
  "scripts": {
    "dev": "TS_NODE_TRANSPILE_ONLY=true nodemon --config nodemon.fastify.json",
    "dev:express": "nodemon --config nodemon.json",
    "dev:fastify": "TS_NODE_TRANSPILE_ONLY=true nodemon --config nodemon.fastify.json",
    "dev:both": "concurrently \"pnpm run dev:express\" \"pnpm run dev:fastify\"",
    "start": "node dist/fastify-server.js",
    "start:express": "node dist/index.js",
    "start:fastify": "node dist/fastify-server.js",
    "build": "tsc",
    "switch-to-fastify": "cp src/index.ts src/index-express-backup.ts && cp src/fastify-server.ts src/index.ts && echo '✅ Switched to Fastify'",
    "setup": "./setup-postgres.sh",
    "setup-db": "psql -U postgres -c \"CREATE DATABASE blackrent;\" 2>/dev/null || echo \"Databáza už existuje\"",
    "backup": "node backup.js",
    "restore": "node restore.js",
    "list-backups": "ls -la backups/",
    "test-data": "node create-test-data.js",
    "test": "ts-node src/__tests__/simple-test-runner.ts",
    "test:routes": "ts-node src/__tests__/simple-test-runner.ts",
    "migrate:auth": "bash scripts/run-migration.sh",
    "seed:auth": "ts-node scripts/seed-auth-system.ts",
    "test:auth": "ts-node scripts/test-auth-system.ts",
    "setup:auth": "npm run migrate:auth && npm run seed:auth && npm run test:auth",
    "r2:setup": "bash scripts/r2-auto-setup.sh",
    "r2": "source .dev.vars 2>/dev/null || true && bash scripts/r2-cli.sh",
    "r2:list": "source .dev.vars 2>/dev/null || true && bash scripts/r2-cli.sh list",
    "r2:stats": "ts-node scripts/r2-simple-stats.ts",
    "r2:auth": "source .dev.vars 2>/dev/null || true && bash scripts/r2-cli.sh auth",
    "r2:advanced": "source .dev.vars 2>/dev/null || true && ts-node scripts/r2-advanced.ts",
    "r2:test": "source .dev.vars 2>/dev/null || true && ts-node scripts/r2-advanced.ts test",
    "r2:analyze": "source .dev.vars 2>/dev/null || true && ts-node scripts/r2-advanced.ts analyze",
    "r2:cleanup": "source .dev.vars 2>/dev/null || true && ts-node scripts/r2-advanced.ts cleanup",
    "test:migration": "bash test-migration.sh",
    "load-test": "node load-test.js",
    "test:performance": "tsx performance-comparison.ts",
    "test:load-comparison": "chmod +x load-test-comparison.sh && ./load-test-comparison.sh",
    "get-token": "tsx get-test-token.ts",
    "perf:all": "chmod +x run-performance-test.sh && ./run-performance-test.sh",
    "perf:simple": "tsx simple-performance-test.ts",
    "test:all": "tsx test-all-functionality.ts",
    "test:functionality": "source .test.env && tsx test-all-functionality.ts"
  },
  "keywords": [
    "blackrent",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.451.0",
    "@aws-sdk/s3-request-presigner": "^3.451.0",
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "^10.3.0",
    "@pdf-lib/fontkit": "^1.1.1",
    "@prisma/client": "^6.17.1",
    "@smithy/node-http-handler": "^4.3.0",
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.9",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.1",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.8",
    "@types/web-push": "^3.6.4",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.9",
    "bullmq": "^5.28.2",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "imap": "^0.8.19",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.2",
    "mailparser": "^3.7.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.15",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.15.0",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.2",
    "puppeteer": "^23.9.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.7.5",
    "sqlite3": "^5.1.7",
    "typescript": "^5.2.2",
    "uuid": "^10.0.0",
    "web-push": "^3.6.7",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/imap": "^0.8.42",
    "@types/mailparser": "^3.4.6",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react-window-infinite-loader": "^1.0.9",
    "autocannon": "^8.0.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.2",
    "prisma": "^6.17.1",
    "ts-node": "^10.9.1",
    "wrangler": "^4.42.2"
  }
}
